{
  "name": "website-livingactive",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "start": "node .output/server/index.mjs",
    "dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 nuxt dev --https --ssl-cert localhost.pem --ssl-key localhost-key.pem",
    "dev:simple": "NODE_TLS_REJECT_UNAUTHORIZED=0 nuxt dev --https",
    "analyze": "nuxi analyze",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare && husky",
    "lint": "eslint --ext \".ts,.vue\" --ignore-path .gitignore .",
    "prepare": "husky",
    "storyblok:pull-components": "storyblok pull-components --space 277886 --path ./storyblok/schema/",
    "storyblok:generate-typescript": "npm run storyblok:pull-components && storyblok generate-typescript-typedefs --sourceFilePaths ./storyblok/schema/components.277886.json --destinationFilePath ./types/storyblok.d.ts"
  },
  "dependencies": {
    "@nuxt/image": "1.7.0",
    "@pinia/nuxt": "0.5.2",
    "@storyblok/nuxt": "6.0.10",
    "@vueuse/core": "10.11.0"
  },
  "devDependencies": {
    "mobile-detect": "1.4.5",
    "@atoms-studio/nuxt-swiftsearch": "0.9.1",
    "@nuxtjs/eslint-config-typescript": "12.1.0",
    "@nuxtjs/i18n": "8.3.3",
    "@nuxtjs/sitemap": "7.2.10",
    "@shopware-pwa/nuxt3-module": "1.0.5",
    "@shopware/api-client": "1.0.1",
    "@types/micromatch": "^4.0.9",
    "autoprefixer": "10.4.19",
    "embla-carousel-vue": "^8.6.0",
    "eslint": "8.57.0",
    "fs-extra": "11.2.0",
    "husky": "9.0.11",
    "micromatch": "4.0.8",
    "nuxt": "3.13.0",
    "nuxt-multi-cache": "3.4.0",
    "path": "0.12.7",
    "pinia": "2.1.7",
    "postcss": "8.4.38",
    "slugify": "1.6.6",
    "storybook": "8.2.9",
    "tailwindcss": "3.4.7",
    "typescript": "5.4.5",
    "uuid": "11.0.2",
    "vite-svg-loader": "5.1.0"
  },
  "engines": {
    "node": "^22.x"
  },
  "overrides": {
    "vue": "latest"
  }
}
