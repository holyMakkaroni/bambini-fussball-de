{
  "name": "LivingActive Theme",
  "author": "uniqueweb",
  "previewMedia": "app/storefront/dist/assets/themePreview.jpg",
  "views": [
    "@Storefront",
    "@Plugins",
    "@LiaTheme"
  ],
  "style": [
    "app/storefront/src/scss/overrides.scss",
    "@Storefront",
    "app/storefront/src/scss/base.scss"
  ],
  "script": [
    "@Storefront"
  ],
  "asset": [
    "@Storefront",
    "app/storefront/src/assets"
  ],
  "configInheritance": [
    "@Storefront",
    "@LiaTheme"
  ],
  "config": {
    "tabs": {
      "settings": {
        "label": {
          "en-GB": "Settings",
          "de-DE": "Einstellungen"
        }
      }
    },
    "blocks": {
      "cookieBot": {
        "label": {
          "en-GB": "Cookiebot",
          "de-DE": "Cookiebot"
        }
      },
      "google": {
        "label": {
          "en-GB": "Google",
          "de-DE": "Google"
        }
      }
    },
    "fields": {
      "cookieBotSettingsId": {
        "label": {
          "en-GB": "Id",
          "de-DE": "Id"
        },
        "type": "text",
        "editable": true,
        "tab": "settings",
        "block": "cookieBot",
        "scss": false
      },
      "googleSiteVerificationId": {
        "label": {
          "en-GB": "Id",
          "de-DE": "Id"
        },
        "type": "text",
        "editable": true,
        "tab": "settings",
        "block": "google",
        "scss": false
      },
      "sw-color-brand-primary": {
        "label": {
          "en-GB": "Primary colour",
          "de-DE": "Primärfarbe"
        },
        "type": "color",
        "value": "#FC4D0F",
        "editable": true,
        "block": "themeColors",
        "order": 100
      },
      "sw-color-brand-secondary": {
        "label": {
          "en-GB": "Secondary colour",
          "de-DE": "Sekundärfarbe"
        },
        "type": "color",
        "value": "#24272C",
        "editable": true,
        "block": "themeColors",
        "order": 200
      },
      "sw-border-color": {
        "label": {
          "en-GB": "Border",
          "de-DE": "Rahmen"
        },
        "type": "color",
        "value": "#F4F4F4",
        "editable": true,
        "block": "themeColors",
        "order": 300
      },
      "sw-color-success": {
        "label": {
          "en-GB": "Success",
          "de-DE": "Erfolg"
        },
        "type": "color",
        "value": "#6FAA0D",
        "editable": true,
        "block": "statusColors",
        "order": 100
      },
      "sw-font-family-base": {
        "label": {
          "en-GB": "Fonttype text",
          "de-DE": "Schriftart Text"
        },
        "type": "fontFamily",
        "value": "'Hind', sans-serif",
        "editable": true,
        "block": "typography",
        "order": 100
      },
      "sw-text-color": {
        "label": {
          "en-GB": "Text colour",
          "de-DE": "Textfarbe"
        },
        "type": "color",
        "value": "$sw-color-brand-secondary",
        "editable": true,
        "block": "typography",
        "order": 200
      },
      "sw-font-family-headline": {
        "label": {
          "en-GB": "Fonttype headline",
          "de-DE": "Schriftart Überschrift"
        },
        "type": "fontFamily",
        "value": "$sw-font-family-base",
        "editable": true,
        "block": "typography",
        "order": 300
      },
      "sw-headline-color": {
        "label": {
          "en-GB": "Headline colour",
          "de-DE": "Überschriftfarbe"
        },
        "type": "color",
        "value": "$sw-color-brand-secondary",
        "editable": true,
        "block": "typography",
        "order": 400
      },
      "sw-logo-desktop": {
        "label": {
          "en-GB": "Desktop",
          "de-DE": "Desktop"
        },
        "helpText": {
          "en-GB": "Displayed on viewport sizes above 991px and as a fallback on smaller viewports, if no other logo is set.",
          "de-DE": "Wird bei Ansichten über 991px angezeigt und als Alternative bei kleineren Auflösungen, für die kein anderes Logo eingestellt ist."
        },
        "type": "media",
        "value": "app/storefront/dist/assets/logo/logo.svg",
        "editable": true,
        "block": "media",
        "order": 100,
        "fullWidth": true
      },
      "sw-logo-tablet": {
        "label": {
          "en-GB": "Tablet",
          "de-DE": "Tablet"
        },
        "helpText": {
          "en-GB": "Displayed between a viewport of 767px to 991px",
          "de-DE": "Wird zwischen einem viewport von 767px bis 991px angezeigt"
        },
        "type": "media",
        "value": "app/storefront/dist/assets/logo/logo.svg",
        "editable": true,
        "block": "media",
        "order": 200,
        "fullWidth": true
      },
      "sw-logo-mobile": {
        "label": {
          "en-GB": "Mobile",
          "de-DE": "Mobil"
        },
        "helpText": {
          "en-GB": "Displayed up to a viewport of 767px",
          "de-DE": "Wird bis zu einem Viewport von 767px angezeigt"
        },
        "type": "media",
        "value": "app/storefront/dist/assets/logo/logo.svg",
        "editable": true,
        "block": "media",
        "order": 300,
        "fullWidth": true
      }
    }
  }
}